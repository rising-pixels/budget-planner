{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier", "import"],
  "rules": {
    "prettier/prettier": "error",
    "import/no-default-export": "error"
  },
  "overrides": [
    {
      "files": ["./src/pages/index.tsx", "./src/pages/_app.tsx"],
      "rules": {
        "import/no-default-export": "off"
      }
    }
  ]
}
